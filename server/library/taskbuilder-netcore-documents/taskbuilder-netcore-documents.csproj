<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <RootNamespace>TaskBuilderNetCore.Documents</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\taskbuilder-netcore-common\taskbuilder-netcore-common.csproj" />
    <ProjectReference Include="..\taskbuilder-netcore-interfaces\taskbuilder-netcore-interfaces.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Controllers\LoaderMessagesStrings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>LoaderMessagesStrings.resx</DependentUpon>
    </Compile>
    <Compile Update="Controllers\OrchestratorMessagesStrings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>OrchestratorMessagesStrings.resx</DependentUpon>
    </Compile>
    <Compile Update="Controllers\StateSerializerMessagesStrings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>StateSerializerMessagesStrings.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Controllers\LoaderMessagesStrings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>LoaderMessagesStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Controllers\OrchestratorMessagesStrings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>OrchestratorMessagesStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Controllers\StateSerializerMessagesStrings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>StateSerializerMessagesStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
