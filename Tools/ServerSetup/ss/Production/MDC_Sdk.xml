<?xml version="1.0" encoding="utf-8"?>
<SetupDefinition xmlns="http://schemas.microarea.it/setup"
                 configuration="debug"
                 upgradeGuid="A1D880AC-CB00-47EA-B9B9-FB61EC0E8A2F"
                 producer="Microarea S.p.A."
                 productName="Microarea MDC SDK"
                 setupOutputFileName="MDC_SDK_Mago4_"
                 >
  <!--Consideriamo questo path come root di partenza: C:\Development\-->
	<FileSystemStructure>
    <IncludeFolder name="Apps">
      <IncludeFolder name="TbApps">
        <IncludeFolder name="Debug|Release">
          <IncludeFile name="MDC*.dll|Microarea.MDC.*.dll" />
        </IncludeFolder>
      </IncludeFolder>
    </IncludeFolder>
		<IncludeFolder name="Standard">
			<IncludeFolder name="Applications">
				<IncludeFolder name="MDC">
					<IncludeFolder name="Core|Core_IT|ElectronicInvoicing_IT|FATEL|Solutions|TaxDocuments_IT|TaxSettlement_IT">
						<IncludeFolder name="*" recursive="yes">
							<IncludeFile name="*.xml|*.sql|*.menu|*.config|*.gif"/>
						</IncludeFolder>
            <IncludeFolder name="JsonForms" recursive="yes">
							<IncludeFile name="*.*json"/>
						</IncludeFolder>
            <IncludeFolder name="ModuleObjects" recursive="yes">
                <IncludeFile name="*.*json|*.xml"/>
            </IncludeFolder>
						<IncludeFolder name="Modules">
							<IncludeFile name="*.csm"/>
						</IncludeFolder>
            <ExcludeFolder name="Microarea.*|DataManager" />
						<IncludeFile name="*.Solution.xml|*.config"/>
					</IncludeFolder>
					<IncludeFile name="Application.config" />
				</IncludeFolder>
			</IncludeFolder>
		</IncludeFolder>
	</FileSystemStructure>
</SetupDefinition>
