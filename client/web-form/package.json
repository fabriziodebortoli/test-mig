{
    "name": "tbweb",
    "version": "0.0.0",
    "license": "MIT",
    "angular-cli": {},
    "scripts": {
        "start": "ng serve",
        "build": "ng build",
        "clean": "rimraf ./node_modules",
        "link-core": "npm link ../core/src",
        "link-erp": "npm link ../erp/src",
        "link-icons": "npm link ../icons/src",
        "link-rs": "npm link ../reporting-studio/src",
        "link-bpm": "npm link ../bpm/src",
        "unlink-all": "npm unlink @taskbuilder/reporting-studio --no-save && npm unlink @taskbuilder/core --no-save && npm unlink @taskbuilder/icons --no-save && npm unlink @taskbuilder/bpm --no-save && npm unlink @taskbuilder/erp --no-save ",
        "preinstall": "del package-lock.json && npm run unlink-all",
        "postinstall": "npm run link-core && npm run link-rs && npm run link-icons && npm run link-erp && npm run link-bpm",
        "lint": "tslint \"src/**/*.ts\"",
        "test": "ng test",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^4.4.4",
        "@angular/cdk": "2.0.0-beta.12",
        "@angular/common": "^4.4.4",
        "@angular/compiler": "^4.4.4",
        "@angular/core": "^4.4.4",
        "@angular/forms": "^4.4.4",
        "@angular/http": "^4.4.4",
        "@angular/material": "2.0.0-beta.12",
        "@angular/platform-browser": "^4.4.4",
        "@angular/platform-browser-dynamic": "^4.4.4",
        "@angular/router": "^4.4.4",
        "@taskbuilder/bpm": "git+https://github.com/Microarea/ng-bpm.git",
        "@taskbuilder/core": "git+https://github.com/Microarea/ng-core.git",
        "@taskbuilder/erp": "git+https://github.com/Microarea/ng-erp.git",
        "@taskbuilder/icons": "git+https://github.com/Microarea/ng-icons.git",
        "@taskbuilder/reporting-studio": "git+https://github.com/Microarea/ng-reporting-studio.git",
        "@types/hammerjs": "^2.0.33",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "lodash": "4.17.4",
        "npm": "^5.4.2",
        "rxjs": "^5.4.2",
        "ts-helpers": "^1.1.1",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular/cli": "1.4.4",
        "@angular/compiler-cli": "^4.4.4",
        "@angular/language-service": "^4.4.4",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "~3.2.0",
        "copy-webpack-plugin": "^4.2.3",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.7.0",
        "typescript": "~2.3.3"
    }
}
