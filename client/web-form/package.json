{
    "name": "tbweb",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "ng serve",
        "build": "ng build",
        "clean": "rimraf ./node_modules",
        "link-core": "npm link ../core/src --no-save",
        "link-erp": "npm link ../erp/src --no-save",
        "link-icons": "npm link ../icons/src --no-save",
        "link-rs": "npm link ../reporting-studio/src --no-save",
        "link-bpm": "npm link ../bpm/src --no-save",
        "unlink-all": "npm unlink @taskbuilder/reporting-studio --no-save && npm unlink @taskbuilder/core --no-save && npm unlink @taskbuilder/icons --no-save && npm unlink @taskbuilder/bpm --no-save && npm unlink @taskbuilder/erp --no-save ",
        "preinstallNO": "npm run unlink-all",
        "postinstall": "npm run link-core && npm run link-rs && npm run link-icons && npm run link-erp && npm run link-bpm",
        "lint": "tslint \"src/**/*.ts\"",
        "test": "ng test",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.0",
        "@angular/cdk": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/material": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "@taskbuilder/bpm": "git+https://github.com/Microarea/ng-bpm.git",
        "@taskbuilder/core": "git+https://github.com/Microarea/ng-core.git",
        "@taskbuilder/erp": "git+https://github.com/Microarea/ng-erp.git",
        "@taskbuilder/icons": "git+https://github.com/Microarea/ng-icons.git",
        "@taskbuilder/reporting-studio": "git+https://github.com/Microarea/ng-reporting-studio.git",
        "@types/hammerjs": "^2.0.33",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "lodash": "4.17.4",
        "npm": "^5.4.2",
        "rxjs": "^5.5.6",
        "ts-helpers": "^1.1.1",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular-devkit/core": "^0.2.0",
        "@angular/cli": "1.6.2",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "^4.0.1",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.5.3"
    }
}
