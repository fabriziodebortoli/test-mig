{
    "compilerOptions": {
        "baseUrl": "./",
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "rootDir": "./",
        "target": "es6",
        "lib": [
            "es2015",
            "dom"
        ],
        "skipLibCheck": true,
        "types": [],
        "paths": {
            "@angular/*": [
                "../../web-form/node_modules/@angular/*"
            ],
            "@taskbuilder/*": [
                "../../web-form/node_modules/@taskbuilder/*"
            ],
            "rxjs/*": [
                "../../web-form/node_modules/rxjs/*"
            ],
            "ngx-gallery/*": [
                "../../web-form/node_modules/ngx-gallery/*"
            ]
        }
    }
}