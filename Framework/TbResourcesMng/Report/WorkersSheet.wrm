		Rules
			From RM_Workers 
			Select 
				Name                 Into w_Name ,
				Disabled             Into w_Disabled ,
				DomicilyAddress      Into w_DomicilyAddress ,
				Telephone1           Into w_Telephone1 ,
				Email                Into w_Email ,
				LastName             Into w_LastName ,
				WorkerID             Into w_WorkerID ,
				RegisterNumber       Into w_RegisterNumber ,
				SkypeID              Into w_SkypeID ,
				Telephone2           Into w_Telephone2 ,
				Telephone3           Into w_Telephone3 ,
				Telephone4           Into w_Telephone4 ,
				ImagePath            Into w_ImagePath ,
				Gender               Into w_Gender ,
				CivilStatus          Into w_CivilStatus ,
				DomicilyCity         Into w_DomicilyCity ,
				DomicilyFC           Into w_DomicilyFC ,
				DomicilyZip          Into w_DomicilyZip ,
				DomicilyCounty       Into DomicilyCounty ,
				DomicilyCountry      Into w_DomicilyCountry ,
				Notes                Into w_WorkerNotes ,
				URL                  Into w_URL ,
				Address2             Into w_Address2 ,
				StreetNo             Into w_StreetNo ,
				District             Into w_District ,
				FederalState         Into w_FederalState 
			Where ( 
				( 
				( w_UseWorkerIdFilter == true ) And ( RM_Workers.WorkerID == w_WorkerIdFilter ) 
				) 
				or 
				( 
				( w_UseWorkerIdFilter == false ) 
				And 
				( 
				( w_askAllWorkers == true ) or ( RM_Workers.WorkerID Between w_askFromWorker And w_askToWorker ) 
				) 
				) 
				)  
			Order By RM_Workers.WorkerID ;

			w_CompleteEmail = "mailto:" + w_Email  ;
		End


