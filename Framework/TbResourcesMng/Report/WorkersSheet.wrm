	Procedures
		Begin
			Procedure  RunQueryWorkerDescription 
				Begin
					F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryReadOne ( "q_WorkerDescription" )  ;
				End

			Procedure  RunQueryWorkersFields 
				Begin
					F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryOpen ( "q_WorkersFields" )  ;
					w_IsFirstTime = true  ;
					While F_ok 
						Do 
							Begin
								F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryRead ( "q_WorkersFields" )  ;
								If ( F_ok and w_IsFirstTime ) 
									Then 
										Begin
											FormFeed "Customdata" ;
											Reset w_WorkerTitle ;
											w_IsFirstTime = false  ;
										End ;
								If F_ok 
									Then 
										Begin
											Call DisplayCustomFieldsRow ;
											NextLine RM_WorkersFields ;
											Call ResetCustomFieldsRow ;
										End ;
							End ;
					F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryClose ( "q_WorkersFields" )  ;
				End

			Procedure  RunQueryWorkersDetails 
				Begin
					F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryOpen ( "q_WorkersDetails" )  ;
					w_IsFirstTime = true  ;
					While F_ok 
						Do 
							Begin
								F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryRead ( "q_WorkersDetails" )  ;
								If ( F_ok and w_IsFirstTime ) 
									Then 
										Begin
											FormFeed "WorkersDetails" ;
											w_IsFirstTime = false  ;
										End ;
								If F_ok 
									Then 
										Begin
											Call DisplayWorkersDetails ;
											NextLine RM_WorkersDetails ;
											Call ResetWorkersDetails ;
										End ;
							End ;
					F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryClose ( "q_WorkersDetails" )  ;
				End

			Procedure  RunQueryWorkersArrangement 
				Begin
					F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryOpen ( "q_WorkersArrangement" )  ;
					w_IsFirstTime = true  ;
					While F_ok 
						Do 
							Begin
								F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryRead ( "q_WorkersArrangement" )  ;
								If ( F_ok and w_IsFirstTime ) 
									Then 
										Begin
											FormFeed "Arrangements" ;
											Reset w_WorkerTitle ;
											w_IsFirstTime = false  ;
										End ;
								If F_ok 
									Then 
										Begin
											Call DisplayWorkersArrangement ;
											NextLine RM_WorkersArrangements ;
											Call ResetWorkersArrangement ;
										End ;
							End ;
					F_ok = Framework.TbWoormViewer.TbWoormViewer.QueryClose ( "q_WorkersArrangement" )  ;
				End

			Procedure  DisplayWorkersFields 
				Begin
					w_WorkerTitle = w_Name + " " + w_LastName  ;
					If ( w_ImagePath == "" ) 
						Then w_WorkerImage = w_EmptyWorkerImage  ;
						Else w_WorkerImage = w_ImagePath  ;
					Display w_WorkerTitle ;
					Display w_Disabled ;
					Display w_LastName ;
					Display w_Name ;
					Display w_DomicilyAddress ;
					Display w_WorkerID ;
					Display w_Email ;
					Display w_ImagePath ;
				End

			Procedure  DisplayCustomFieldsRow 
				Begin
					w_WorkerTitle = w_Name + " " + w_LastName + Localize ( ": Custom Data" )  ;
					Display w_WorkerTitle ;
					Display w_FieldName ;
					Display w_FieldValue ;
					Display w_HideOnLayout ;
				End

			Procedure  ResetCustomFieldsRow 
				Begin
					Reset w_FieldName ;
					Reset w_FieldValue ;
					Reset w_HideOnLayout ;
					Reset w_WorkerTitle ;
				End

			Procedure  DisplayWorkersArrangement 
				Begin
					w_WorkerTitle = w_Name + " " + w_LastName + Localize ( ": Arrangement" )  ;
					Display w_WorkerTitle ;
					Display w_Arrangement ;
					Display w_ArrangementLevel ;
					Display w_BasicPay ;
					Display w_TotalPay ;
					Display w_FromDate ;
					Display w_ToDate ;
					Display w_WorkersArrangementNotes ;
					Display w_ArrangmentWorkerID ;
					Display w_Notes ;
				End

			Procedure  ResetWorkersArrangement 
				Begin
					Reset w_Arrangement ;
					Reset w_ArrangementLevel ;
					Reset w_BasicPay ;
					Reset w_TotalPay ;
					Reset w_FromDate ;
					Reset w_ToDate ;
					Reset w_WorkersArrangementNotes ;
					Reset w_ArrangmentWorkerID ;
					Reset w_Notes ;
					Reset w_WorkerTitle ;
				End

		Procedure  DisplayWorkersDetails 
				Begin
					w_WorkerTitle = w_Name + " " + w_LastName + Localize ( ": Resources" )  ;
					Display WorkerId_RM_WorkersDetails ;
					w_WorkerDescri = w_LastName + " " + w_Name  ;
					Display w_WorkerDescri ;
					Display ChildResourceType ;
					Display ChildResourceCode ;
					Display ChildWorkerID ;
					Call RunQueryWorkerDescription ;
					w_ChildWorkerDescri = LastName + " " + Name  ;
					Display w_ChildWorkerDescri ;
				End

		Procedure  ResetWorkersDetails 
				Begin
					Reset WorkerId_RM_WorkersDetails ;
					Reset w_WorkerDescri ;
					Reset ChildResourceType ;
					Reset ChildResourceCode ;
					Reset ChildWorkerID ;
					Reset w_ChildWorkerDescri ;
				End

		Procedure  DisplayWorker 
				Begin
					Call DisplayWorkersFields ;
					Call RunQueryWorkersFields ;
					Call RunQueryWorkersDetails ;
					Call RunQueryWorkersArrangement ;
				End
		End

